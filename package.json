{
  "name": "ddv-util",
  "version": "0.0.11",
  "description": "ddv-util",
  "main": "./lib/util.js",
  "scripts": {
    "lint": "eslint --ext .js lib time --ignore-pattern dist",
    "dist": "npm run lint&&webpack",
    "precommit": "npm run dist",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ddvjs/ddv-util.git"
  },
  "keywords": [
    "ddv-util",
    "util",
    "filter"
  ],
  "author": "hua",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ddvjs/ddv-util/issues"
  },
  "jshintConfig": {
    "esversion": 6,
    "undef": true,
    "unused": true,
    "node": true,
    "browser": true,
    "predef": [
      "module",
      "require",
      "console",
      "process"
    ],
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-debugger": 0,
    "no-console": 2
  },
  "homepage": "https://github.com/ddvjs/ddv-util#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.10.2",
    "eslint-plugin-html": "^1.3.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "webpack": "^2.2.0"
  }
}
